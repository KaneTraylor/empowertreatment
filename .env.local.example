# Calendar Integration
# Options: 'custom' | 'calendly' | 'google'
NEXT_PUBLIC_CALENDAR_TYPE=custom

# Calendly Configuration (if using Calendly)
NEXT_PUBLIC_CALENDLY_URL=https://calendly.com/your-organization/welcome-call

# Google Calendar Configuration (if using Google)
NEXT_PUBLIC_GOOGLE_CALENDAR_ID=your-calendar-id@group.calendar.google.com
NEXT_PUBLIC_GOOGLE_API_KEY=your-google-api-key
NEXT_PUBLIC_GOOGLE_APPOINTMENT_URL=https://calendar.google.com/calendar/u/0/appointments/schedules/YOUR_SCHEDULE_ID

# Twilio Configuration
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# SendGrid Configuration
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@empowertreatment.com

# Email Recipients (comma-separated for multiple)
NOTIFICATION_EMAILS=admin@empowertreatment.com,team@empowertreatment.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key